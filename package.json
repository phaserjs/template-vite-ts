{
  "name": "template-vite-ts",
  "description": "A Phaser 3 TypeScript template using Vite.",
  "version": "1.2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-vite-ts.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-vite-ts/issues"
  },
  "homepage": "https://github.com/phaserjs/template-vite-ts#readme",
  "scripts": {
    "delete-assets": "node src/GranadaExporters/tasks/delete-assets.mjs",
    "zip": "node src/GranadaExporters/tasks/zip-directory.mjs",
    "compressHtml": "node src/GranadaExporters/tasks/compress-html-file.mjs",
    "dev": "cross-env VITE_DEV_MODE=true WIDTH=375 HEIGHT=812 ENCODE_AUDIO=true vite --config vite/config.dev.ts",
    "build": "cross-env VITE_DEV_MODE=false WIDTH=375 HEIGHT=812 ENCODE_AUDIO=true vite build --config vite/config.prod.ts && pnpm delete-assets && pnpm compressHtml && pnpm zip"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@types/yargs": "^17.0.32",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "html-minifier-terser": "^7.2.0",
    "jszip": "^3.10.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5",
    "svgo": "^3.2.0",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-singlefile": "^2.0.1",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "phaser": "^3.80.1"
  }
}
